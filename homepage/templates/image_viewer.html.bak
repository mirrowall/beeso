   <div id="fullscreen"></div>
   <script type="text/juicer" id="fullscreentpl">
    <div class="fullscreen">
        <div class="fullscreen-close-btn icon-btn"></div>
        {@if !(pn === lowerBound && spn === 0)}
        <div class="left-arrow-wrap arrow-wrap">
            <div class="left-arrow arrow-icon icon-btn slider-arrow"></div>
        </div>
        {@/if}
        {@if !(pn === upperBound && spn === currTotal - 1)}
        <div class="right-arrow-wrap arrow-wrap">
            <div class="right-arrow arrow-icon icon-btn slider-arrow"></div>
        </div>
        {@/if}
        <div class="content">
            <img src="${curr.thumbURL}" class="fullscreen-img"{@if curr.contentStyle} style="width:${curr.contentStyle.width}px;height:${curr.contentStyle.height}px"{@/if}>
        </div>
        <div class="slider" style="{@if unfold != 'unfold'}bottom:-105px;{@/if}">
            <div class="slider-info">
                <div class="slider-toolbar">
                    <span class="unfold info-bar unfold-bar {@if unfold != 'unfold'}hide{@/if}"><i class="toolbar-icon toggle-icon icon-btn"></i><span>收起列表</span></span>
                    <span class="unfold-switch info-bar unfold-bar {@if unfold == 'unfold'}hide{@/if}"><i class="toolbar-icon toggle-icon unfold-icon icon-btn"></i><span>展开列表</span></span>
                    <span class="play info-bar play-bar {@if play != 'play'}hide{@/if}"><i class="toolbar-icon play-icon icon-btn"></i><span>自动播放</span></span>
                    <span class="pause info-bar play-switch play-bar {@if play != 'play-switch'}hide{@/if}"><i class="toolbar-icon pause-icon icon-btn"></i><span>暂停</span></span>
                </div>
                <span class="progress" style="{@if currTotal <= 1}display:none{@/if}">${currIndex}/${currTotal}</span>
            </div>
            <div class="list-wrapper">
                <div class="list" style="left:${offset}px">
                {@each list as it}
                <div class="item ${it.thumbClass}{@if pn == it.pn && spn == it.spn} active{@/if}" data-pn="${it.pn}" data-spn="${it.spn}" style="width:${it.thumbWidth}px">
                    <div class="slider-img-wrap">
                        <img src="${it.thumbURL}" class="slider-img" data-pn="${it.pn}" data-spn="${it.spn}">
                    </div>
                    <div class="slider-img-mask"></div>
                    <div class="album" style="{@if it.bdSetImgNum <= 0}display:none{@/if}">
                        <span class="album-icon"></span>
                        <span class="album-mount">${it.bdSetImgNum}</span>
                    </div>
                </div>
                {@/each}
                </div>
                {@if !surrounding.noPrev}
                <div class="list-arrow-wrap left-list-arrow-wrap">
                    <div class="left-arrow arrow-icon icon-btn list-arrow"></div>
                </div>
                {@/if}
                {@if !surrounding.noNext}
                <div class="list-arrow-wrap right-list-arrow-wrap">
                    <div class="right-arrow arrow-icon icon-btn list-arrow"></div>
                </div>
                {@/if}
            </div>
        </div>
    </div>
</script>

